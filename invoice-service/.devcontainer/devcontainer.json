{
  "name": "Go",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "features": {
    "ghcr.io/devcontainers/features/docker-in-docker:2": {
      "version": "latest"
    }
  },
  "postCreateCommand": "kind create cluster --name demo-procurement-solution || true",
  "mounts": [
    {
      "type": "volume",
      "source": "invoice-service",
      "target": "/home/vscode"
    }
  ],
  "customizations": {
    "devpod": {
      "prebuildRepository": "fdog239/prebuilds"
    },
    "vscode": {
      "settings": {},
      "extensions": [
        "streetsidesoftware.code-spell-checker"
      ]
    }
  },
  "forwardPorts": [
    
        4500
    
  ],
  "portsAttributes": {
    
        "4500": {
          "label": "Hello Remote World",
          "onAutoForward": "notify"
        }
    
  }
}